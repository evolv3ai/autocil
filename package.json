{
  "name": "autocil",
  "version": "0.1.0",
  "description": "A CLI tool that generates teamocil YAML configurations for tmux sessions",
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "packageManager": "pnpm@10.7.0",
  "bin": {
    "autocil": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "format": "prettier --write \"*.{js,mjs,ts,json,md,mdx,yml,yaml}\" \"src/**/*.{js,mjs,jsx,ts,tsx,json,md,mdx,css,yml,yaml}\""
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@types/iarna__toml": "^2.0.5",
    "@types/node": "^20.17.22",
    "@types/yaml": "^1.9.7",
    "prettier": "^3.5.3",
    "typescript": "^5.3.3"
  }
}
